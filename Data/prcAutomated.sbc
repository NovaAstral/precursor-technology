          <CubeSize>Small</CubeSize>
          <GuiVisible>false</GuiVisible>
          <BlockTopology>TriangleMesh</BlockTopology>
          <Size x="1" y="1" z="1" />
          <ModelOffset x="0" y="0" z="0" />
          <Model>Models\Cubes\Small\Sensor.mwm</Model>

          <Components>
						<Component Subtype="PrecursorAlloy" Count="50" >
							<DeconstructId>
								<TypeId>Ore</TypeId>
