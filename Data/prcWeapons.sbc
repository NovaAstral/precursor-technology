                <NoAmmoSoundName>WepRocketLaunchNoAmmo</NoAmmoSoundName>
                <ReloadSoundName>WepRocketLaunchReload</ReloadSoundName>

                <DeviateShotAngle>0</DeviateShotAngle>
                <DeviateShotAngleAiming>0</DeviateShotAngleAiming>
                
                <AmmoMagazines>
                    <AmmoMagazine Subtype="LargePrecursorRailgunAmmo" />
                </AmmoMagazines>

                <ReloadTime>120</ReloadTime>

                <RecoilJetpackVertical>0</RecoilJetpackVertical>
                <RecoilJetpackHorizontal>0</RecoilJetpackHorizontal>
                <RecoilGroundVertical>0</RecoilGroundVertical>
                <RecoilGroundHorizontal>0</RecoilGroundHorizontal>

                <RecoilMultiplierDataNames>
                    <string>Standing</string>
                    <string>Walking</string>
                    <string>Running</string>
                    <string>Crouching</string>
                    <string>Aiming</string>
                </RecoilMultiplierDataNames>

                <RecoilMultiplierDataVerticals>
                    <float>1</float>
                    <float>1</float>
                    <float>1.5</float>
                    <float>.75</float>
                    <float>.75</float>
                </RecoilMultiplierDataVerticals>

                <RecoilMultiplierDataHorizontals>
                    <float>1</float>
                    <float>1</float>
                    <float>1.5</float>
                    <float>.75</float>
                    <float>.75</float>
                </RecoilMultiplierDataHorizontals>

                <ShootDirectionUpdateTime>200</ShootDirectionUpdateTime>

                <EquipDuration>0.5</EquipDuration>

                <ShakeOnActionPrimary>true</ShakeOnActionPrimary>
                <ShakeOnActionSecondary>false</ShakeOnActionSecondary>
                <ShakeOnActionTertiary>false</ShakeOnActionTertiary>
        </Weapon>

		<!-- Small Missile Launcher-->
            <Weapon>
                <Id>
                    <TypeId>WeaponDefinition</TypeId>
                    <SubtypeId>SmallPrecursorMissileLauncher</SubtypeId>
                </Id>

                <MissileAmmoData RateOfFire="500" ShootSoundName="WepShipSmallMissileShot" ShotsInBurst="0" />

                <DeviateShotAngle>0</DeviateShotAngle>
                <DeviateShotAngleAiming>0</DeviateShotAngleAiming>

                <NoAmmoSoundName>WepShipGatlingNoAmmo</NoAmmoSoundName>

                <AmmoMagazines>
                    <AmmoMagazine Subtype="PrecursorMissile" />
                </AmmoMagazines>

                <ReloadTime>1000</ReloadTime>
            </Weapon>

        <!-- Large Missile Launcher-->
            <Weapon>
                <Id>
                    <TypeId>WeaponDefinition</TypeId>
                    <SubtypeId>LargePrecursorMissileLauncher</SubtypeId>
                </Id>

                <MissileAmmoData RateOfFire="500" ShootSoundName="WepShipSmallMissileShot" ShotsInBurst="19" />

                <DeviateShotAngle>0</DeviateShotAngle>
                <DeviateShotAngleAiming>0</DeviateShotAngleAiming>

                <NoAmmoSoundName>WepShipGatlingNoAmmo</NoAmmoSoundName>

                <AmmoMagazines>
                    <AmmoMagazine Subtype="PrecursorMissile" />
                </AmmoMagazines>

                <ReloadTime>1000</ReloadTime>
            </Weapon>

        <!-- Gatling Gun -->
            <Weapon>
                <Id>
                    <TypeId>WeaponDefinition</TypeId>
                    <SubtypeId>PrecursorGatlingGun</SubtypeId>
                </Id>

                <ProjectileAmmoData RateOfFire="1000" ShootSoundName="WepShipGatlingShot" ShotsInBurst="37000"/>

                <ReleaseTimeAfterFire>204</ReleaseTimeAfterFire>

                <DeviateShotAngle>0</DeviateShotAngle>

                <RangeMultiplier>5</RangeMultiplier>
                <DamageMultiplier>158174201</DamageMultiplier>

                <MuzzleFlashLifeSpan>2</MuzzleFlashLifeSpan>
                
                <NoAmmoSoundName>WepShipGatlingNoAmmo</NoAmmoSoundName>
                <SecondarySoundName>WepShipGatlingRotation</SecondarySoundName>
            
                <AmmoMagazines>
                    <AmmoMagazine Subtype="NATO_25x184mm"/>
                </AmmoMagazines>

                <ReloadTime>500</ReloadTime>
            </Weapon>

            <!-- Large Railgun -->
            <Weapon>
                <Id>
                    <TypeId>WeaponDefinition</TypeId>
                    <SubtypeId>LargePrecursorRailgun</SubtypeId>
                </Id>
                <MissileAmmoData RateOfFire="20" ShootSoundName="WepRailgunLargeShot" PreShotSoundName="WepRailgunLargeCharge" FlightSoundName="HeavyShellFlightSound" ShotsInBurst="1" />
                <DeviateShotAngle>0</DeviateShotAngle>
                <ShotDelay>100</ShotDelay>
                <DeviateShotAngleAiming>0</DeviateShotAngleAiming>
                <NoAmmoSoundName>WepShipRailgunNoAmmo</NoAmmoSoundName>
                <AmmoMagazines>
                    <AmmoMagazine Subtype="LargePrecursorRailgunAmmo" />
                </AmmoMagazines>
                <ReloadTime>120</ReloadTime>
                <Effects>
                    <Effect Action="BeforeShoot" Dummy="barrel_001" Particle="Muzzle_Flash_RailgunLarge" Loop="false" InstantStop="false" OffsetX="0" OffsetY="0" OffsetZ="-9"/>
                </Effects>
            </Weapon>

            <!-- Small Railgun -->
            <Weapon>
                <Id>
                    <TypeId>WeaponDefinition</TypeId>
