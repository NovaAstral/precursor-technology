                <DeviateShotAngle>0</DeviateShotAngle>
                <ShotDelay>100</ShotDelay>
                <DeviateShotAngleAiming>0</DeviateShotAngleAiming>
                <NoAmmoSoundName>WepShipRailgunNoAmmo</NoAmmoSoundName>
                <AmmoMagazines>
                    <AmmoMagazine Subtype="SmallPrecursorRailgunAmmo" />
                </AmmoMagazines>
                <ReloadTime>120</ReloadTime>
                <Effects>
                    <Effect Action="BeforeShoot" Dummy="barrel_001" Particle="Muzzle_Flash_RailgunSmall" Loop="false" InstantStop="false" OffsetX="0" OffsetY="0.1" OffsetZ="-1.8"/>
                </Effects>
            </Weapon>
		<!-- prcWeapons END -->
  </Weapons>
</Definitions>